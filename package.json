{
  "name": "gesiklo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prerender": "http-server -c-1 dist/browser",
    "dev:ssr": "ng run gesiklo:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run gesiklo:server:production",
    "prerender": "ng run gesiklo:prerender",
    "build:functions": "npm run --prefix functions build",
    "build:all": "npm run build:ssr && npm run build:functions"
  },
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "tls": false
  },
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.2.1",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/platform-server": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@firebase/firestore": "^2.1.7",
    "@nguniversal/express-engine": "^11.2.0",
    "bootstrap": "^4.6.0",
    "dns": "^0.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "firebase": "^8.2.9",
    "jquery": "^3.5.1",
    "localstorage-polyfill": "^1.0.1",
    "nodemailer": "^6.4.18",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@nguniversal/builders": "^11.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.6.4",
    "@types/node": "^12.20.4",
    "bufferutil": "^4.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
